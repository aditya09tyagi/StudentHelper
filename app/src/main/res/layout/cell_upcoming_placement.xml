<?xml version="1.0" encoding="utf-8"?><!--suppress AndroidUnknownAttribute -->
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="@dimen/padding_12_dp"
    android:layout_marginTop="@dimen/padding_8_dp"
    android:layout_marginEnd="@dimen/padding_12_dp"
    android:elevation="@dimen/padding_4_dp"
    app:cardCornerRadius="@dimen/padding_4_dp"
    app:cardUseCompatPadding="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/classDetailsHolder"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_14_dp"
            android:layout_marginTop="@dimen/padding_14_dp"
            android:layout_marginEnd="@dimen/padding_14_dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/subjectNameTv"
                fontPath="fonts/Lato-Medium.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:alpha="0.96"
                android:textColor="@color/black"
                android:textSize="@dimen/font_14_sp"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingPrefix"
                tools:text="Physics"
                tools:textStyle="bold" />

            <TextView
                android:id="@+id/videoNameTv"
                fontPath="fonts/Lato-Bold.ttf"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/padding_14_dp"
                android:maxLines="2"
                android:textColor="@color/black"
                android:textSize="@dimen/font_16_sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/subjectNameTv"
                tools:ignore="MissingPrefix"
                tools:text="Newton’s First Law - Newton’s Laws of Motion - Of Three Motions"
                tools:textStyle="bold" />

            <TextView
                android:id="@+id/tutorNameTv"
                fontPath="fonts/Lato.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/padding_12_dp"
                android:alpha="0.88"
                android:maxLines="2"
                android:textColor="@color/black"
                android:textSize="@dimen/font_16_sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/videoNameTv"
                tools:ignore="MissingPrefix"
                tools:text="Tutor - Harsh Patodia" />

            <TextView
                android:id="@+id/tutorInstitutionTv"
                fontPath="fonts/Lato.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/padding_4_dp"
                android:alpha="0.56"
                android:maxLines="2"
                android:textColor="@color/black"
                android:textSize="@dimen/font_14_sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tutorNameTv"
                tools:ignore="MissingPrefix"
                tools:text="(IIT Delhi)" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/tutorPhotoIv"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:src="@color/light_grey"
                app:civ_border_color="@color/light_grey"
                app:civ_border_width="@dimen/padding_2_dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/videoNameTv"
                tools:ignore="ContentDescription" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/registrationDetailsHolder"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/padding_20_dp"
            android:background="@color/physics_color_light"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/classDetailsHolder"
            tools:layout_height="wrap_content">

            <TextView
                android:id="@+id/startDateTv"
                fontPath="fonts/Lato-Medium.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/padding_14_dp"
                android:layout_marginTop="@dimen/padding_8_dp"
                android:layout_marginBottom="@dimen/padding_14_dp"
                android:textColor="#000000"
                android:textSize="@dimen/font_14_sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingPrefix"
                tools:text="12th October, 2019" />

            <TextView
                android:id="@+id/interestedStudentsCountTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/padding_14_dp"
                android:layout_marginTop="@dimen/padding_12_dp"
                android:layout_marginBottom="@dimen/padding_16_dp"
                android:alpha="0.80"
                android:textColor="@color/black"
                android:textSize="@dimen/font_12_sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/startDateTv"
                tools:text="3245 students participated" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>