<?xml version="1.0" encoding="utf-8"?><!--suppress AndroidUnknownAttribute -->
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="@dimen/padding_12_dp"
    android:layout_marginTop="@dimen/padding_8_dp"
    android:layout_marginEnd="@dimen/padding_12_dp"
    android:elevation="@dimen/padding_4_dp"
    app:cardCornerRadius="@dimen/padding_4_dp"
    app:cardUseCompatPadding="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/classCoverIv"
            android:layout_width="0dp"
            android:layout_height="110dp"
            android:background="@color/lighter_grey"
            android:scaleType="centerCrop"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="ContentDescription" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/classDetailsHolder"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/padding_14_dp"
            android:layout_marginTop="@dimen/padding_14_dp"
            android:layout_marginEnd="@dimen/padding_14_dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/classCoverIv">

            <TextView
                android:id="@+id/subjectNameTv"
                fontPath="fonts/Lato-Medium.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:alpha="0.96"
                android:textColor="@color/black"
                android:textSize="@dimen/font_14_sp"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingPrefix"
                tools:text="Physics"
                tools:textStyle="bold" />


            <TextView
                android:id="@+id/videoNameTv"
                fontPath="fonts/Lato-Bold.ttf"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/padding_14_dp"
                android:maxLines="2"
                android:textColor="@color/black"
                android:textSize="@dimen/font_16_sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/subjectNameTv"
                tools:ignore="MissingPrefix"
                tools:text="Newton’s First Law - Newton’s Laws of Motion - Of Three Motions"
                tools:textStyle="bold" />

            <TextView
                android:id="@+id/tutorNameTv"
                fontPath="fonts/Lato.ttf"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/padding_12_dp"
                android:alpha="0.88"
                android:maxLines="2"
                android:textColor="@color/black"
                android:textSize="@dimen/font_16_sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/videoNameTv"
                tools:ignore="MissingPrefix"
                tools:text="Tutor - Harsh Patodia (IIT Delhi)" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/registrationDetailsHolder"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/padding_20_dp"
            android:background="@color/physics_color_light"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/classDetailsHolder"
            tools:layout_height="wrap_content">

            <TextView
                android:id="@+id/startsOnTv"
                fontPath="fonts/Lato-Medium.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/padding_14_dp"
                android:layout_marginTop="@dimen/padding_8_dp"
                android:alpha="0.56"
                android:text="@string/starts_on_text"
                android:textColor="#000000"
                android:textSize="@dimen/font_14_sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingPrefix" />

            <TextView
                android:id="@+id/startDateTv"
                fontPath="fonts/Lato-Medium.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/padding_14_dp"
                android:layout_marginTop="@dimen/padding_8_dp"
                android:layout_marginBottom="@dimen/padding_14_dp"
                android:textColor="#000000"
                android:textSize="@dimen/font_14_sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/startsOnTv"
                tools:ignore="MissingPrefix"
                tools:text="09.30 PM , 12th Oct" />

            <TextView
                android:id="@+id/interestedStudentsCountTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/padding_small"
                android:alpha="0.80"
                android:padding="@dimen/padding_micro"
                android:textColor="@color/black"
                android:textSize="@dimen/font_medium_small"
                android:visibility="visible"
                app:layout_constraintTop_toBottomOf="@id/startDateTv"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                tools:text="3245 students interested" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/registerStatusLayout"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:padding="@dimen/padding_normal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/registerButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:padding="@dimen/padding_medium"
                    android:text="@string/register_btn_text"
                    android:textColor="@color/white"
                    android:textSize="@dimen/font_14_sp"
                    android:visibility="gone"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:background="@drawable/bg_rounded_corner" />

                <TextView
                    android:id="@+id/registeredTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawablePadding="@dimen/padding_medium"
                    android:padding="@dimen/padding_medium"
                    android:text="@string/registered_text"
                    android:textSize="@dimen/font_14_sp"
                    android:visibility="gone"
                    tools:visibility="visible"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:textColor="@color/physics_color" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>